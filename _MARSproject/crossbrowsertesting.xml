<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<!-- For parallel testing put this in suite level parallel = "tests"-->
<suite name="Suite" thread-count="5" parallel="tests">
<listeners>
<listener class-name="com.rims.extentreportlistener.ExtentReportManager"></listener>
</listeners>
  <test  name="ChromTest">
  <parameter name="os" value="windows"/>
  <parameter name="browser" value="chrome"/>
  
    <classes>
    <class name="RIA.LogIn"/>
      <!-- <class name="RIA.Returnapproval"/> -->
      <class name="com.rims.testcases.DispositionTestcase"/> 
      <class name="com.rims.testcases.BrandcallLogTest"/>
    </classes>
  </test> <!-- Test -->
  
  
  <test name="EdgeTest">
  <parameter name="os" value="windows"/>
  <parameter name="browser" value="edge"/>
  
    <classes>
    <class name="RIA.LogIn"/>
      <!-- <class name="RIA.Returnapproval"/> -->
      <class name="com.rims.testcases.DispositionTestcase"/> 
      <class name="com.rims.testcases.BrandcallLogTest"/>
    </classes>
  </test> <!-- Test -->
  
  
  <test  name="FirefoxTest">
  <parameter name="os" value="windows"/>
  <parameter name="browser" value="firefox"/>
  
    <classes>
    <class name="RIA.LogIn"/>
      <!-- <class name="RIA.Returnapproval"/> -->
      <class name="com.rims.testcases.DispositionTestcase"/> 
      <class name="com.rims.testcases.BrandcallLogTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
